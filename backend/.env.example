# Database
DATABASE_URL=postgresql://user:password@localhost/dbname

# Snapshots
SNAPSHOT_DIR=../snapshots

# Matching Parameters
MATCH_MAX_DIST_PX=80.0
FUSE_TIME_WINDOW=2.0
ASSOC_WEIGHT_SIDE=1.0
ASSOC_WEIGHT_TOP=0.6
ASSOC_TIME_DECAY=3.0

# Model
MODEL_PATH=../models/yolov8s-seg_openvino_model/
TARGET_FPS=12
INFERENCE_WIDTH=480
ENABLE_MASKS=1
ALPHA=0.35
USE_HALF=1
TORCH_THREADS=1

# Motion & Display
DISPLAY_FPS=15
MOTION_DETECTION_WIDTH=320
MOTION_THRESHOLD=0.02
MOTION_HIGH_FPS=8
MOTION_LOW_FPS=1

# RTSP Streams
RTSP_URL_1=rtsp://admin:admin123456@192.168.1.6:554/ch=1?subtype=1
RTSP_URL_2=rtsp://admin:admin123456@192.168.1.4:554/ch=1?subtype=1

# Homography
HOMOGRAPHY_PATH=../homography_top_to_side.npy

# WebRTC (ICE)
STUN_URL=stun:stun.l.google.com:19302

# FFmpeg Relay
START_FFMPEG_RELAY=0
OUTPUT_URL_1=rtmp://localhost/live/cam1
OUTPUT_URL_2=rtmp://localhost/live/cam2
